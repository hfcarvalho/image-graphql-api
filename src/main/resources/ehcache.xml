<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ehcache>
<ehcache>

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
        name="defaut"
        maxElementsInMemory="10000"
        eternal="false"
        timeToIdleSeconds="3600"
        timeToLiveSeconds="3600"
        overflowToDisk="true"
        diskPersistent="true"
        memoryStoreEvictionPolicy="LRU"
    />

    <cache name="images"
           maxElementsInMemory="10000"
    />
    
    <cache name="persons"
           maxElementsInMemory="10000"
    />

    <cache name="folders"
           maxElementsInMemory="10000"
    />

</ehcache>